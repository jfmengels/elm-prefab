{
  "app": {},
  "routes": {
    "Home": "/old-homepage-2/:id/*?{search}",
    "Markdown": {
      "dir": "guide",
      "url": "/guides/*",
      "urlOnServer": "/assets/guides/"
    }
  },
  "interactive": {
    "output": "./interactive",
    "modules": ["Ui.Button"]
  },
  "theme": {
    "colors": {
      "white": "#ffffff",
      "black": "#000000",
      "neutral": {
        "50": "#fafafa",
        "100": "#f5f5f5",
        "200": "#e5e5e5",
        "300": "#d4d4d4",
        "400": "#a3a3a3",
        "500": "#737373",
        "600": "#525252",
        "700": "#404040",
        "800": "#262626",
        "900": "#171717",
        "950": "#0a0a0a"
      },
      "primary": {
        "50": "#fdf2f8",
        "100": "#fce7f3",
        "200": "#fbcfe8",
        "300": "#f9a8d4",
        "400": "#f472b6",
        "500": "#ec4899",
        "600": "#db2777",
        "700": "#be185d",
        "800": "#9d174d",
        "900": "#831843",
        "950": "#500724"
      }
    },
    "palettes": {
      "primary": {
        "background": "#ec4899",
        "foreground": "#f5f5f5",
        "border": "#ec4899"
      },
      "secondary": {
        "background": "#ffffff",
        "foreground": "#262626",
        "border": "#d4d4d4"
      },
      "danger": {
        "background": "#ec4899",
        "foreground": "#f5f5f5",
        "border": "#ec4899"
      },
      "neutral": {
        "background": "#ffffff",
        "foreground": "#000000",
        "border": "#000000"
      },
      "neutralInverted": {
        "background": "#000000",
        "foreground": "#ffffff",
        "border": "#000000"
      }
    },
    "spacing": {
      "xxSmall": 2,
      "xSmall": 4,
      "small": 8,
      "medium": 16,
      "large": 32,
      "xLarge": 64
    },
    "typography": {
      "h1": {
        "face": "EB Garamond",
        "fallback": ["serif"],
        "size": 24,
        "color": "#171717"
      },
      "h2": {
        "face": "EB Garamond",
        "fallback": ["serif"],
        "size": 20,
        "color": "#171717"
      },
      "default": {
        "face": "Noto Sans",
        "fallback": ["sans-serif"],
        "size": 16,
        "color": "#171717"
      },
      "small": {
        "face": "Noto Sans",
        "fallback": ["sans-serif"],
        "size": 10,
        "color": "#171717"
      }
    },
    "borders": {
      "small": { "rounded": 2, "width": 1 }
    },
    "shadows": []
  }
}
