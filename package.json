{
  "name": "elm-press",
  "version": "1.0.0",
  "description": "A simple markdown -> Elm code generator",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen:init": "elm-codegen init",
    "gen:install": "(cd plugins/app ; elm-codegen install) ; (cd plugins/theme ; elm-codegen install)",
    "gen:run": "ts-node ./cli/generate.ts",
    "editor:install": "cd editor; elm-codegen install",
    "editor:run": "cd editor; elm-codegen run src/Generate.elm --debug --flags-from=test-apps/button.json --output=generated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdgriffith/elm-press.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mdgriffith/elm-press/issues"
  },
  "homepage": "https://github.com/mdgriffith/elm-press#readme",
  "devDependencies": {
    "@types/node": "^15.0.3",
    "@types/node-fetch": "^2.5.10",
    "elm": "0.19.1-5",
    "elm-codegen": "^0.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.4"
  }
}
