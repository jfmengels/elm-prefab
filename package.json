{
  "name": "elm-prefab",
  "version": "0.1.0",
  "description": "Maintainable Codegeneration for Elm Apps",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bash scripts/build.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gen:install": "(cd plugins ; elm-codegen install) ; (cd plugins/app ; elm-codegen install) ; (cd plugins/theme ; elm-codegen install) ; (cd plugins/interactive ; elm-codegen install)",
    "gen:run": "pnpm run build ; cd ./examples/elm-gql ; ts-node ../../cli/run.ts generate",
    "example": "pnpm run gen:run ; (cd examples/elm-gql; elm make src/Main.elm --output=/dev/null)",
    "editor:install": "cd editor; elm-codegen install",
    "editor:run": "cd editor; elm-codegen run src/Generate.elm --debug --flags-from=test-apps/button.json --output=generated"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdgriffith/elm-prefab.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mdgriffith/elm-prefab/issues"
  },
  "homepage": "https://github.com/mdgriffith/elm-prefab#readme",
  "dependencies": {
    "elm-dev": "^0.1.2"
  },
  "devDependencies": {
    "@types/node": "^15.0.3",
    "@types/node-fetch": "^2.5.10",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "elm": "0.19.1-5",
    "elm-codegen": "^0.5.0",
    "elm-optimize-level-2": "^0.3.5",
    "esbuild": "^0.19.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.2.4"
  }
}
